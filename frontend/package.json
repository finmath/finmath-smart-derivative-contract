{
  "name": "sdc-frontend",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "generate:api": "openapi-generator-cli generate -g typescript-angular -i ../src/main/resources/api.yml -o ./src/app/openapi",
